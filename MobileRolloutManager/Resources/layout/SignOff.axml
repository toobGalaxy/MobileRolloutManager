                    android:layout_column="1"
                    android:id="@+id/notes"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
                <TextView
                    android:layout_column="2"
                    android:text="Notes"
                    android:textAppearance="?android:attr/textAppearanceMedium"
                    android:layout_width="match_parent"
