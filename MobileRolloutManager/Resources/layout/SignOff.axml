<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TableLayout
        android:minWidth="25px"
        android:minHeight="25px"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/tableLayout1">
        <TableRow
            android:id="@+id/tableRow1">
            <Button
                android:text="Back"
                android:layout_column="0"
                android:id="@+id/SignOffBack" />
            <Button
                android:text="Add Signature"
                android:layout_column="1"
                android:id="@+id/sigBtn"
                android:background="@drawable/log"
                android:layout_gravity="top"
                android:layout_height="80.5dp"
                android:layout_marginLeft="13.0dp" />
            <Button
                android:text="Submit"
                android:layout_column="2"
                android:id="@+id/Save"
                android:layout_marginLeft="21.5dp" />
        </TableRow>
        <TableRow
            android:id="@+id/tableRow2">
            <TextView
                android:text="Signatures Added = 0"
                android:textAppearance="?android:attr/textAppearanceSmall"
                android:layout_column="2"
                android:id="@+id/addedSignatures"
                android:layout_marginLeft="1.0dp"
                android:layout_marginRight="13.5dp" />
        </TableRow>
    </TableLayout>
    <TextView
        android:text="Medium Text"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:id="@+id/SignOffHeader"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" />
    <TextView
        android:text="Text"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/SignOffText"
        android:layout_marginBottom="6.5dp" />
    <ScrollView
        android:minWidth="25px"
        android:minHeight="25px"
